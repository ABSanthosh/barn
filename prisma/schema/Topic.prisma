model Topic {
  id       String @id @default(cuid())
  category String
  title    String
  xmlUrl   String
  desc     String @db.String(1000)
  type     String

  users UserTopic[]
}

model UserTopic {
  id      String @id @default(cuid())
  userId  String  
  topicId String
  User    User   @relation(fields: [userId], references: [id])
  Topic   Topic  @relation(fields: [topicId], references: [id])
}
