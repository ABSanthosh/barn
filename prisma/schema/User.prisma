model User {
  id           String    @id @default(cuid())
  googleId     String    @unique
  email        String    @unique
  name         String
  picture      String?
  newUser      Boolean   @default(true)
  accessToken  String
  refreshToken String
  idToken      String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @updatedAt

  cities     City[]
  userTopics UserTopic[]
}

